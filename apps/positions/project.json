{
  "name": "positions",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/positions",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["apps/positions/.next"],
      "options": {
        "commands": [
          "next build apps/positions && mv apps/positions/.next/static apps/positions/.next/standalone/apps/positions/.next && cp -r apps/positions/public apps/positions/.next/standalone/apps/positions/.next"
        ]
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "positions:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "positions:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "positions:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "positions:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/apps/positions"],
      "options": {
        "jestConfig": "apps/positions/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/positions/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
