{
  "name": "og",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/og",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "next build apps/og",
          "mv apps/og/.next/static apps/og/.next/standalone/apps/og/.next",
          "cp -r apps/og/public apps/og/.next/standalone/apps/og/.next"
        ],
        "outputPath": "apps/og/.next"
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "og:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "og:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "og:build:production",
          "dev": false
        }
      }
    },
    "serve-standalone": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["node apps/og/.next/standalone/apps/og/server.js"]
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "og:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/og/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/og/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
