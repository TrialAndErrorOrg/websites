{
  "name": "trialanderror.org",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/trialanderror.org",
  "projectType": "application",
  "targets": {
    "build": {
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/trialanderror.org",
        "outputPath": "dist/apps/trialanderror.org"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/trialanderror.org"
        },
        "production": {}
      },
      "command": "next build apps/trialanderror.org && mv apps/trialanderror.org/.next/static apps/trialanderror.org/.next/standalone/apps/trialanderror.org/.next && cp -r apps/trialanderror.org/public apps/trialanderror.org/.next/standalone/apps/trialanderror.org/.next"
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["next dev apps/trialanderror.org -p 4200"]
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "trialanderror.org:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "trialanderror.org:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "trialanderror.org:build:production",
          "dev": false
        }
      }
    },
    "serve-standalone": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "node apps/trialanderror.org/.next/standalone/apps/trialanderror.org/server.js"
        ]
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "trialanderror.org:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/trialanderror.org/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/trialanderror.org/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
